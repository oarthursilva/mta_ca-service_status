{
  "name": "mta_bs-statusoverview_cms",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/eslint-plugin-ui5-jsdocs": "^2.0.5",
    "@sap/xssec": "^4",
    "@sapui5/ts-types": "^1.92.2",
    "eslint": "^7.32.0",
    "express": "^4"
  },
  "engines": {
    "node": ">=22"
  },
  "devDependencies": {
    "@cap-js/cds-test": "^0.4.0",
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": ">=8",
    "cds-plugin-ui5": "^0.13.0"
  },
  "scripts": {
    "build": "cds build --production",
    "start": "cds-serve",
    "test": "npx jest */*.test.js",
    "watch-statusoverview": "cds watch --open statusoverview/index.html?sap-ui-xx-viewCache=false --livereload false",
    "watch-statusoverviewadmin": "cds watch --open statusoverviewadmin/index.html?sap-ui-xx-viewCache=false --livereload false"
  },
  "cds": {
    "requires": {
      "auth": {
        "[development]": {
          "strategy": "mock",
          "users": {
            "user": {
              "roles": [
                "user"
              ]
            },
            "admin": {
              "roles": [
                "admin"
              ]
            }
          }
        }
      },
      "[production]": {
        "db": "hana",
        "auth": {
          "kind": "xsuaa"
        }
      },
      "destinations": true,
      "html5-runtime": true,
      "workzone": true
    },
    "odata": {
      "version": "v4"
    }
  },
  "workspaces": [
    "app/*"
  ],
  "sapux": [
    "app/statusoverview",
    "app/statusoverviewadmin"
  ]
}